<label for="dialogs">Dialog:</label>
<BR>
<table id="dialogs" class="tablesorter">
    <thead>
        <tr>
            <th>character</th>
            <th>text</th>
        </tr>
    </thead>
    <tbody>
    {{#each lines}}
        <tr>
            <td contenteditable="true">{{this.character}}</td>
            <td contenteditable="true">{{this.text}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>
